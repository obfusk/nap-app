.container
  .btn-toolbar.pull-right
    %a.btn{ href: r(:log, app[:name], app[:log], app[:n]) }
      %i.icon-refresh
      Refresh
  .page-header
    %h1
      %a{ href: r(:app, app[:name]) }&= app[:name]
      \/ log /
      &= app[:log]
  %table.table.table-bordered.table-hover
    %tbody
      - log.each_with_index do |x,i|
        %tr
          %td.line-numbers&= i
          %td.w99
            %pre&= x
